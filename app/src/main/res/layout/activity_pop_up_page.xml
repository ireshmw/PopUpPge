<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_pop_up_page"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.learntv.iresh.popuppge.popUpPage">


        <ImageView
            android:layout_width="196dp"
            android:layout_height="19dp"
            app:srcCompat="@drawable/footer"
            android:id="@+id/imageView4"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="@+id/motherBtn"
            android:layout_marginTop="191dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/motherBtn" />

        <ImageButton
            android:layout_width="46dp"
            android:layout_height="0dp"
            app:srcCompat="@drawable/father"
            android:id="@+id/fatherBtn"
            android:background="@drawable/father"
            android:scaleType="fitXY"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="@+id/imageView4"
            android:layout_marginEnd="106dp"
            app:layout_constraintRight_toRightOf="@+id/textView"
            android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginBottom="14dp"
            android:layout_marginRight="106dp" />

        <ImageButton
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:id="@+id/motherBtn"
            android:background="@drawable/mother"
            app:layout_constraintRight_toLeftOf="@+id/fatherBtn"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/sonBtn"
            android:layout_marginEnd="8dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintTop_toTopOf="@+id/fatherBtn"
            app:layout_constraintLeft_toRightOf="@+id/sonBtn"
            android:layout_marginLeft="4dp"
            android:layout_marginRight="8dp" />

        <ImageButton
            android:layout_width="33dp"
            android:layout_height="89dp"
            app:srcCompat="@drawable/son"
            android:id="@+id/sonBtn"
            android:background="@drawable/son"
            android:scaleType="fitXY"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="47dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView4"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="14dp"
            app:layout_constraintLeft_toLeftOf="@+id/textView"
            android:layout_marginLeft="47dp" />

        <ImageButton
            android:layout_width="0dp"
            android:layout_height="82dp"
            app:srcCompat="@drawable/daughter"
            android:id="@+id/daughterBtn"
            android:background="@drawable/daughter"
            android:scaleType="fitXY"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="2dp"
            app:layout_constraintBottom_toBottomOf="@+id/fatherBtn"
            android:layout_marginEnd="10dp"
            app:layout_constraintRight_toRightOf="@+id/imageView4"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toRightOf="@+id/fatherBtn"
            android:layout_marginLeft="2dp"
            android:layout_marginRight="10dp" />

        <TextView
            android:text="Members of the Family"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView"
            tools:gravity="center"
            android:freezesText="false"
            android:forceHasOverlappingRendering="false"
            android:textAppearance="@style/TextAppearance.AppCompat.Headline"
            android:typeface="monospace"
            android:textSize="24sp"
            android:fontFamily="sans-serif"
            android:lineSpacingExtra="18sp"
            android:textStyle="normal|bold"
            android:textColor="@android:color/holo_blue_dark"
            android:textAlignment="center"
            android:textAllCaps="true"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="16dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/memberText"
            android:textStyle="normal|bold"
            android:textSize="24sp"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="26dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:srcCompat="@android:drawable/ic_media_ff"
            android:id="@+id/nextPageBtn"
            android:elevation="2dp"
            android:adjustViewBounds="false"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="9dp"
            app:layout_constraintBottom_toBottomOf="parent"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="@+id/fatherBtn"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="9dp" />

        <ImageButton
            android:layout_width="90dp"
            android:layout_height="90dp"
            app:srcCompat="@android:drawable/ic_menu_rotate"
            android:id="@+id/replayBtn"
            android:elevation="2dp"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="98dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginEnd="98dp"
            app:layout_constraintRight_toRightOf="@+id/nextPageBtn"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="98dp"
            android:layout_marginRight="98dp" />

</android.support.constraint.ConstraintLayout>
